{
    "name": "lambster",
    "version": "1.0.0",
    "description": "Lambda calculus interpreter web interface",
    "main": "webpack.config.js",
    "scripts": {
        "deploy": "./bin/minify",
        "build": "npx webpack --config webpack.config.js",
        "watch": "npx webpack --mode=development --watch --config webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/minchingtonak/lambster.git"
    },
    "author": "Alec Minchington",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/minchingtonak/lambster/issues"
    },
    "homepage": "https://github.com/minchingtonak/lambster#readme",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.9.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "@types/node": "^14.0.6",
        "@types/react-dom": "^16.9.8"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@types/react": "^16.9.35",
        "lambda": "file:../lambda",
        "npm-force-resolutions": "0.0.3",
        "prop-types": "^15.7.2",
        "react": "^15.1.1",
        "react-dom": "^15.1.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 2,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    }
}
